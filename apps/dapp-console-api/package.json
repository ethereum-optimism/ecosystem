{
  "name": "@eth-optimism/dapp-console-api",
  "private": true,
  "version": "0.0.0",
  "main": "src/index.ts",
  "types": "types/index.d.ts",
  "module": "build/index.mjs",
  "scripts": {
    "test": "vitest --passWithNoTests",
    "clean": "rm -rf build types tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "typecheck:ci": "tsc --noEmit --emitDeclarationOnly false",
    "dev": "tsup --watch --onSuccess 'pnpm kill && pnpm start'",
    "kill": "npx kill-port 7300",
    "build": "tsup && pnpm tsc",
    "start": "node build/cmd/run.js",
    "docker:services:up": "docker compose up -d redis",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "lint": "eslint \"**/*.{ts,tsx}\" && pnpm prettier --check \"**/*.{ts,tsx}\"",
    "lint:ci": "eslint \"**/*.{ts,tsx}\" --quiet && pnpm prettier --check \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix --quiet && pnpm prettier \"**/*.{ts,tsx}\" --write --loglevel=warn"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "4.17.41",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/qs": "6.9.11",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.53.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "vitest": "^1.1.3"
  },
  "dependencies": {
    "@trpc/server": "^10.45.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-prom-bundle": "^7.0.0",
    "iron-session": "^8.0.1",
    "morgan": "^1.10.0",
    "pino": "^8.19.0",
    "prom-client": "^15.1.0",
    "superjson": "^1.12.2",
    "trpc-playground": "^1.0.4",
    "zod": "^3.22.4"
  },
  "files": [
    "build"
  ]
}
