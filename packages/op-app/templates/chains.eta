    ...chainConfig,
    id: <%= chainDef.chain_id %>,
    name: '<%= chainDef.name %>',
    nativeCurrency: {
        decimals: 18,
        name: 'Ether',
        symbol: 'ETH',
    },
    rpcUrls: {
        default: {
            http: ['<%= chainDef.public_rpc %>'],
        },
    },
    blockExplorers: {
        default: {
            name: 'Blockscout',
            url: '<%= chainDef.explorer %>',
        },
    },
    sourceId: <%= chainDef.source_chain_id %>
})

<% }) %>
